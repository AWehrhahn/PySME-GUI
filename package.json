{
  "name": "pysme-gui",
  "version": "0.2.5",
  "description": "A GUI for PySME",
  "repository": "https://github.com/AWehrhahn/PySME-GUI",
  "homepage": "https://github.com/AWehrhahn/PySME-GUI",
  "author": "Ansgar Wehrhahn",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "sass": "sass scss/index.scss css/index.css",
    "watch": "tsc -w",
    "make": "electron-forge make",
    "start": "tsc && electron-forge start",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-dmg": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@types/plotly.js": "^1.50.1",
    "@types/tmp": "^0.1.0",
    "electron": "^8.2.2",
    "sass": "^1.26.3",
    "tslint": "^5.19.0",
    "typescript": "^3.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "acorn": "^6.4.1",
    "apache-arrow": "^0.16.0",
    "base64-arraybuffer": "^0.2.0",
    "bootstrap": "^4.4.1",
    "chokidar": "^3.3.1",
    "citation-js": "^0.5.0-alpha.5",
    "datatables.net": "^1.10.20",
    "datatables.net-bs4": "^1.10.20",
    "datatables.net-responsive-bs4": "^2.2.3",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.5.0",
    "plotly.js": "^1.54.1",
    "popper.js": "^1.16.1",
    "tmp": "^0.1.0",
    "untildify": "^4.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "PySmeGUI"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {}
        }
      ]
    }
  }
}
